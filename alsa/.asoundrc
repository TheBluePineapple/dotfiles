##make sure you use the right card
defaults.pcm.card 1
defaults.ctl.card 1
#utilize the correct card
pcm.!default {
	type hw
	card 1
	device 0
}
ctl.!default {
	type hw
	card 1
}
#this one is necessary for mocp to work
pcm.!default {
    type pipewire
    playback_node "-1"
    capture_node  "-1"
    hint {
        show on
        description "Default ALSA Output (currently PipeWire Media Server)"
    }
}
